<script>
    $('.logout').click(function () {
        $.get("/com/logout", function (res, status) {
            if (res.code != 200) {
                Swal.fire({
                    icon: 'error',
                    // title: '提示信息',
                    text: res.message,
                })
            } else {
                Swal.fire({
                    icon: 'success',
                    html: '退出成功！',
                    timer: 2000,
                    showConfirmButton: false,
                    timerProgressBar: true,
                    didOpen: () => {
                        Swal.hideLoading()
                    },
                }).then((result) => {
                    window.location.reload()
                })
            }
        });
    })

</script>
